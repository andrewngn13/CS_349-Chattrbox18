.formId = formId;\n    this.inputId = inputId;\n  }\n  init(submitCallback) {\n    $(this.formId).submit((event) => {\n      event.preventDefault();\n      let val = $(this.inputId).val();\n      submitCallback(val);\n      $(this.inputId).val('');\n    });\n    $(this.formId).find('button').on('click', () => {\n      $(this.formId).submit();\n    });\n  }\n}\n\nexport class ChatList {\n  constructor(listId, username) {\n    this.listId = listId;\n    this.odd = false;\n    this.username = username;\n  }\n  drawMessage(messageData) {\n    var $messageRow = $('<li>', {\n      class: 'message-row'\n    });\n    var $message = $('<p>');\n    $message.append($('<span>', {\n      class: 'message-username',\n      text: messageData.user\n    }));\n    $message.append($('<span>', {\n      class: 'timestamp',\n      'data-time': messageData.timestamp\n    }));\n    $message.append($('<span>', {\n      class: 'message-message',\n      text: messageData.message\n    }));\n    $messageRow.append($message);\n\n    var $img = $('<img>', {\n      src: createGravatarUrl(messageData.user),\n      title: messageData.user\n    });\n    $messageRow.append($img);\n\n    $(this.listId).append($messageRow);\n    $messageRow.get(0).scrollIntoView();\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"ba425165ce1f77d450134b028785a6c8","cacheData":{"env":{}}}